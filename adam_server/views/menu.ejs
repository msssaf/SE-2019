	<!-- USING ESCAPE VELOCITY TEMPLATE FOR LOGO AND NAVBAR--> 
	<link rel="stylesheet" href="assets_escape_velocity/css/main.css" />
		<body class="homepage is-preload">
			<div id="page-wrapper">

				<!-- Header -->
					<section id="header" class="wrapper">

						<!-- Logo -->
							<div id="logo">
								<h1><a href="/">Codename Adam</a></h1>
							</div>

						<!-- Nav -->
							<nav id="nav">
								<ul>
									<li class="current"><a href="/">Home</a></li>
									<li>
										<a href="/menu">Menu</a>
										<ul>
											<li><a href="/popular-dishes">Most Popular</a></li>
											<li><a href="/appetizers">Appetizers</a></li>
											<li><a href="/entrees">Entrees</a></li>
											<li><a href="/drinks">Drinks</a></li>
											<li><a href="/desserts">Desserts</a></li>
										</ul>
									</li>
									<li><a href="/recommend-me">Recommend Me</a></li>
									<li><a href="/cart">Cart</a></li>
									<li><a href="/reserve">Reserve</a></li>
									<li><a href="#">Your Profile</a></li>
								</ul>
							</nav>

					</section>
			</div>

			<!-- Scripts -->
				<script src="assets_escape_velocity/js/jquery.min.js"></script>
				<script src="assets_escape_velocity/js/jquery.dropotron.min.js"></script>
				<script src="assets_escape_velocity/js/browser.min.js"></script>
				<script src="assets_escape_velocity/js/breakpoints.min.js"></script>
				<script src="assets_escape_velocity/js/util.js"></script>
				<script src="assets_escape_velocity/js/main.js"></script>

		</body>


	<!-- USING PHANTOM TEMPLATE FOR DISPLAYING MENU ITEMS -->
	<link rel="stylesheet" href="assets_phantom/css/main.css" />
	<noscript><link rel="stylesheet" href="assets_phantom/css/noscript.css" /></noscript>
		<body class="is-preload">
			<!-- Wrapper -->
				<div id="wrapper">

					<!-- Main -->
						<div id="main">
							<div class="inner">
								<!-- DISPLAY 3 POPULAR DISHES-->
								<header> 
									<br>
									<h1><a href="/popular-dishes">Most Popular Dishes</a><br /></h1>
								</header>
								<section class="tiles">

									<% for(var i = 0; i < pop_dishes.length; i++){ %>
										<!-- DISPLAYS MENU USING TILES -->
										<article class="style5">
											<span class="image">
												<img src=<%="../public/images/menu/"+pop_dishes[i].filename%> alt="" style="width:auto;height:325px;"/>
											</span>
											<a href="#">
												<div class="content style3">
													<h2 style="color:#FFFFFF;"><%=pop_dishes[i].title%></h2>
													<p><%=pop_dishes[i].description%> <br>
													$<%=pop_dishes[i].price%></p>
													<button onClick="addToCart();">Add</button>
												</div>
											</a>
										</article>

									<% } %>
								</section>

								<!-- DISPLAY 3 APPETIZERS -->
								<header> 
									<br>
									<h1><a href="/appetizers">Appetizers</a><br /></h1>
								</header>
								<section class="tiles">
									<!-- DISPLAYS MENU USING TILES -->
									<% for(var i = 0; i < appetizers.length; i++){ %>
										<!-- DISPLAYS MENU USING TILES -->
										<article class="style5">
											<span class="image">
												<img src=<%="../public/images/menu/"+appetizers[i].filename%> alt="" style="width:auto;height:325px;"/>
											</span>
											<a href="#">
												<div class="content style3">
													<h2 style="color:#FFFFFF;"><%=appetizers[i].title%></h2>
													<p><%=appetizers[i].description%> <br>
													$<%=appetizers[i].price%></p>
													<button onClick="">Add</button>
													<button onClick="">Remove</button>
												</div>
											</a>
										</article>

									<% } %>
								</section>

								<!-- DISPLAY 3 ENTREES-->
								<header> 
									<br>
									<h1><a href="/entrees">Entrees</a><br /></h1>
								</header>
								<section class="tiles">
