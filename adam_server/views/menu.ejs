											<span class="image">
												<img src=<%="../public/images/menu/"+drinks[i].filename%> alt="" style="width:auto;height:325px;"/>
											</span>
											<a href="#">
												<div class="content style3">
													<h2 style="color:#FFFFFF;"><%=drinks[i].title%></h2>
													<p><%=drinks[i].description%> <br>
													$<%=drinks[i].price%></p>
													<button onClick="">Add</button>
													<button onClick="">Remove</button>
												</div>
											</a>
										</article>

									<% } %>
								</section>
							</div>
						</div>

				</div>

			<!-- PHANTOM Scripts -->
				<script src="assets_phantom/js/jquery.min.js"></script>
				<script src="assets_phantom/js/browser.min.js"></script>
				<script src="assets_phantom/js/breakpoints.min.js"></script>
				<script src="assets_phantom/js/util.js"></script>
				<script src="assets_phantom/js/main.js"></script>
		</body>
</html>