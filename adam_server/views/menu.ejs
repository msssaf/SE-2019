													<h2 style="color:#FFFFFF;"><%=entrees[i].title%></h2>
													<p><%=entrees[i].description%> <br>
													$<%=entrees[i].price%></p>
													<button onClick="">Add</button>
													<button onClick="">Remove</button>
												</div>
											</a>
										</article>

									<% } %>
								</section>

								<!-- DISPLAY 3 DESSERTS -->
								<header> 
									<br>
									<h1><a href="/desserts">Desserts</a><br /></h1>
								</header>
								<section class="tiles">
									<% for(var i = 0; i < desserts.length; i++){ %>
										<!-- DISPLAYS MENU USING TILES -->
										<article class="style5">
											<span class="image">
												<img src=<%="../public/images/menu/"+desserts[i].filename%> alt="" style="width:auto;height:325px;"/>
											</span>
											<a href="#">
												<div class="content style3">
													<h2 style="color:#FFFFFF;"><%=desserts[i].title%></h2>
													<p><%=desserts[i].description%> <br>
													$<%=desserts[i].price%></p>
													<button onClick="">Add</button>
													<button onClick="">Remove</button>
												</div>
											</a>
										</article>

									<% } %>
								</section>

								<!-- DISPLAY 3 DRINKS -->
								<header> 
									<br>
									<h1><a href="/drinks">Drinks</a><br /></h1>
								</header>
								<section class="tiles">
									<% for(var i = 0; i < drinks.length; i++){ %>
										<!-- DISPLAYS MENU USING TILES -->
