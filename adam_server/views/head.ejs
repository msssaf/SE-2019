<html>
	<head>
		<title>Codename Adam</title>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
		<link rel="stylesheet" href="assets_escape_velocity/css/main.css" />
	</head>
	<body class="homepage is-preload">
		<div id="page-wrapper">

			<!-- Header -->
				<section id="header" class="wrapper">

					<!-- Logo -->
						<div id="logo">
							<h1><a href="/">Codename Adam</a></h1>
						</div>

					<!-- Nav -->
						<nav id="nav">
							<ul>
								<li class="current"><a href="/">Home</a></li>
								<li>
									<a href="/menu">Menu</a>
									<ul>
										<li><a href="/popular-dishes">Most Popular</a></li>
										<li><a href="/appetizers">Appetizers</a></li>
										<li><a href="/entrees">Entrees</a></li>
										<li><a href="/drinks">Drinks</a></li>
										<li><a href="/desserts">Desserts</a></li>
									</ul>
								</li>
								<li><a href="/recommend-me">Recommend Me</a></li>
								<li><a href="/cart">Cart</a></li>
								<li><a href="/reserve">Reserve</a></li>
								<% if (user) { %>
									<% if (user.role != 0) {%>
								<li><a href="/profile">Your Profile</a></li>
								<li><a href="/logout">Log Out</a></li>
									<% } else if (user.role == 0) { %>
								<li><a href="/console">Admin Console</a></li>
								<li><a href="/profile">Your Profile</a></li>
								<li><a href="/logout">Log Out</a></li>	
									<% } %>					
								<% } else { %>
								<li><a href="/login">Log In</a></li>
								<li><a href="/register">Register</a></li>
								<% } %>
							</ul>
						</nav>

				</section>
		</div>

		<!-- Scripts -->
			<script src="assets_escape_velocity/js/jquery.min.js"></script>
			<script src="assets_escape_velocity/js/jquery.dropotron.min.js"></script>
